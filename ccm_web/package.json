{
  "name": "ccm_web",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev:server": "nodemon --inspect=0.0.0.0:9229 --config nmon_config.json server/src/main.ts",
    "dev:client": "webpack --watch --config webpack/webpack.dev.ts",
    "predev": "wait-port ccm_db:3306",
    "dev": "node -r ts-node/register server/src/migrator up && npm run dev:server & npm run dev:client",
    "build:client": "webpack --config webpack/webpack.prod.ts",
    "build:server": "tsc --project tsconfig.build.json",
    "preprod": "npm run build:client && npm run build:server",
    "prod": "NODE_ENV=production node dist/server/src/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./server/test/jest-e2e.json --detectOpenHandles"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/sequelize": "^0.2.0",
    "@nestjs/swagger": "^4.8.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "connect-session-sequelize": "^7.1.1",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "got": "^11.8.2",
    "keycode-js": "^3.1.0",
    "ltijs": "~5.7.3",
    "ltijs-sequelize": "~2.4.3",
    "notistack": "^1.0.5",
    "papaparse": "^5.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.6",
    "sequelize": "^6.3.3",
    "sequelize-typescript": "^2.1.0",
    "swagger-ui-express": "^4.1.6",
    "umzug": "^3.0.0-beta.15",
    "wait-port": "^0.2.9",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.6.15",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.1",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/html-webpack-plugin": "^3.2.4",
    "@types/jest": "^26.0.20",
    "@types/material-ui": "^0.21.8",
    "@types/node": "^14.14.31",
    "@types/papaparse": "^5.2.5",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.10",
    "@types/webpack": "^4.41.27",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.1.1",
    "eslint": "^7.22.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "html-webpack-plugin": "^5.2.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "style-loader": "^2.0.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-merge": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "server/src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}

    "@mui/material": "5.15.15",
    "@nestjs/axios": "3.1.0",
    "@nestjs/common": "10.4.6",
    "@nestjs/config": "3.3.0",
    "@nestjs/core": "10.4.6",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.4.6",
    "@nestjs/sequelize": "10.0.1",

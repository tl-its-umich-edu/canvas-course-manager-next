    "@nestjs/swagger": "7.4.2",
    "@nestjs/terminus": "10.2.3",
    "@react-hook/debounce": "4.0.0",
    "axios": "1.7.7",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "connect-session-sequelize": "7.1.7",
    "cookie-parser": "1.4.7",
    "csrf-csrf": "3.0.8",
